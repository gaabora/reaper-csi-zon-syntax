{
  "name": "reaper-csi-zon-syntax",
  "repository": "todo",
  "displayName": "REAPER CSI ZON Syntax Highlighting",
  "description": "Syntax highlighting for .zon files used in Control Surface Integrator for REAPER",
  "version": "1.0.0",
  "publisher": "yourname",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vscode/vsce": "^3.3.2"
  },

  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zon",
        "aliases": ["ZON", "zon"],
        "extensions": [".zon"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zon",
        "scopeName": "source.zon",
        "path": "./zon.tmLanguage.json"
      }
    ]
  }
}
